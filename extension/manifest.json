{
  "manifest_version": 3,
  "name": "DepreCity Scraper",
  "description": "One-click scrape of real estate data.",
  "version": "1.1",
  "permissions": ["activeTab", "scripting", "downloads"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Scrape Listing"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
